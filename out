        # We could change this to use superset image postgresql client.
        - name: wait-for-examples
          image: docker.io/bitnami/postgresql:16.4.0-debian-12-r14
          imagePullPolicy: IfNotPresent
          resources:
            limits:
              cpu: 150m
              ephemeral-storage: 2Gi
              memory: 192Mi
